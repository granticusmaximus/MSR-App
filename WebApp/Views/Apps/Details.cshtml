@model WebApp.Models.AppList

@{
    ViewData["Title"] = "Details";
}

<div class="jumbotron">
    <h1>@Html.DisplayFor(model => model.AppName)'s Details</h1>
</div>


<div>
    <hr />
    <dl class="row">
        <dt class="col-sm-2">
            @Html.DisplayNameFor(model => model.AppName)
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.AppName)
        </dd>
        <dt class="col-sm-2">
            @Html.DisplayNameFor(model => model.Analyst)
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.Analyst.BAFullname)
        </dd>
        <dt class="col-sm-2">
            @Html.DisplayNameFor(model => model.Developer)
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.Developer.DevFullname)
        </dd>
        <dt class="col-sm-2">
            @Html.DisplayNameFor(model => model.POC)
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.POC)
        </dd>
        <dt class="col-sm-2">
            @Html.DisplayNameFor(model => model.Telephone)
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.Telephone)
        </dd>
        <dt class="col-sm-2">
            @Html.DisplayNameFor(model => model.POCEmail)
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.POCEmail)
        </dd>
        <dt class="col-sm-2">
            @Html.DisplayNameFor(model => model.AppNotes)
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.AppNotes)
        </dd>
    </dl>
</div>
<div>
    <a asp-action="Edit" asp-route-id="@Model.AppID">Edit</a> |
    <a asp-action="Index">Back to List</a>
</div>
